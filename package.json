    "lint": "eslint --ext '**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix --ext '**/*.{ts,tsx}'",
    "format": "prettier --write --config .prettierrc.json '**/*.{ts,tsx,md}'"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": "https://github.com/ryanleonbutler/toy_robot/issues",
